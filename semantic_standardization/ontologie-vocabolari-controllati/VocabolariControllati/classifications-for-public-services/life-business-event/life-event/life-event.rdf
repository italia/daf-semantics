  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/life-event/5">
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/life-event"/>
    <skos:definition xml:lang="it">Questo evento della vita raggruppa servizi pubblici relativi alla guida di un veicolo per esempio una macchina, una moto</skos:definition>
    <dct:identifier>5</dct:identifier>
    <skos:prefLabel xml:lang="en">Applying to a driver's licence (or renewing an existing one)</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/italia/onto/CPSV/LifeEvent"/>
    <skos:prefLabel xml:lang="it">Richiesta o rinnovo patente</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>5</skos:notation>
    <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
    <skos:definition xml:lang="en">This life event groups public services related to driving a vehicle, for instance car, motorcycle.</skos:definition>
    <skos:prefLabel xml:lang="de">Beantragung eines Führerscheins (oder Erneuerung eines bestehenden Führerscheins)</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/life-event/4">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">This life event groups public services related to when someone retires from his job or becomes a senior.</skos:definition>
    <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
    <skos:notation>4</skos:notation>
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/life-event"/>
    <dct:identifier>4</dct:identifier>
    <skos:definition xml:lang="it">Questo evento della vita raggruppa servizi pubblici relativi al pensionamento di una persona</skos:definition>
    <skos:prefLabel xml:lang="de">Pensionierung</skos:prefLabel>
    <skos:prefLabel xml:lang="it">Pensionamento</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/italia/onto/CPSV/LifeEvent"/>
    <skos:prefLabel xml:lang="en">Retirement</skos:prefLabel>
  </rdf:Description>
